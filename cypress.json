{
  "baseUrl": "https://api.openweathermap.org",
  "reporterEnabled": "mochawesome , @reportportal/agent-js-cypress, mocha-junit-reporter",
  "reporter": "@reportportal/agent-js-cypress",
    "reporterOptions": {
      "endpoint": "http://reportportal.xxx.local:8080/api/v1",
      "token": "xxx",
      "launch": "POINTER",
      "project": "POINTERRP_PERSONAL",
      "description": "Pointer test results in RP"
    
    }, 
  
  "retries": {
    "runMode": 3,
    "openMode": 2
  },
  "responseTimeOut": 60000,
  "requestTimeout": 60000,
  "testFiles": "**/*{feature,test.js}",
  "ignoreTestFiles":"**/playground/*",
  "trashAssetsBeforeRuns": true,
  "viewportHeight": 768,
  "viewportWidth": 1024,
  "projectId": "wmsdoh",
  "chromeWebSecurity": false
}
